<?xml version="1.0" encoding="UTF-8" ?>
<dependencies>
    
  <!-- Android -->
  <androidPackages>

    <!-- Support Library v4 -->
    <androidPackage spec="com.android.support:support-v4:__ANDROID_SUPPORT_LIB_VERSION__">
      <androidSdkPackageIds>
        <androidSdkPackageId>extra-google-m2repository</androidSdkPackageId>
      </androidSdkPackageIds>
    </androidPackage>

    <!-- Urban Airship FCM -->
    <androidPackage spec="com.urbanairship.android:urbanairship-fcm:__ANDROID_AIRSHIP_VERSION__">
      <repositories>
        <repository>https://urbanairship.bintray.com/android</repository>
      </repositories>
    </androidPackage>

  </androidPackages>

  <!-- iOS -->
  
  <iosPods>
      
    <!-- iosPod -->
    <iosPod name="Airship" version="13.1" minTargetSdk="11.0" />
    <iosPod name="Airship/Location" version="13.1" minTargetSdk="11.0" />
  
  </iosPods>

</dependencies>
