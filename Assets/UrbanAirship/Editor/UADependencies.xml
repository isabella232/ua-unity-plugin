<?xml version="1.0" encoding="UTF-8" ?>
<dependencies>

  <!-- Android -->
  <androidPackages>
    <!-- GCM -->
    <androidPackage spec="com.google.android.gms:play-services-gcm:__ANDROID_PLAY_SERVICES_VERSION__">
      <androidSdkPackageIds>
        <androidSdkPackageId>extra-google-m2repository</androidSdkPackageId>
      </androidSdkPackageIds>
    </androidPackage>

    <!-- Support Library v4 -->
    <androidPackage spec="com.android.support:support-v4:__ANDROID_SUPPORT_LIB_VERSION__">
      <androidSdkPackageIds>
        <androidSdkPackageId>extra-google-m2repository</androidSdkPackageId>
      </androidSdkPackageIds>
    </androidPackage>

    <!-- Urban Airship SDK -->
    <androidPackage spec="com.urbanairship.android:urbanairship-sdk:__ANDROID_AIRSHIP_VERSION__">
      <repositories>
        <repository>https://urbanairship.bintray.com/android</repository>
      </repositories>
    </androidPackage>

    <!-- Urban Airship Android Unity Plugin Support -->
    <androidPackage spec="com.urbanairship.android:unity-plugin:__PLUGIN_VERSION__">
      <repositories>
        <repository>https://urbanairship.bintray.com/android</repository>
      </repositories>
    </androidPackage>

  </androidPackages>

</dependencies>
