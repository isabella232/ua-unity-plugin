<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.firebasecompat">

    <application>
        <service
            android:name="com.urbanairship.unity.firebasecompat.CompatMessagingService"
            android:exported="false">
            <intent-filter android:priority="999">
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>


        <service
            android:name="com.urbanairship.unity.firebasecompat.CompatInstanceIdService"
            android:exported="false">
            <intent-filter android:priority="999">
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT" />
            </intent-filter>
        </service>
    </application>
</manifest>
